package 은행입금처리예제;

/*
 * 계좌클래스
 */

public class Account extends Thread{

	private int balance =0;
			
	//메소드 구현
	//예금을 처리하는 메소드
		
	public void add(int deposit) {
		int current_amount = balance ; // 현재 잔액을 읽기
		System.out.println(Thread.currentThread().getName()+
				":현재잔액="+ current_amount+"가산액="+deposit);
	
		
	}
}
